const age = 33;
age >=18 ? console.log('I like wine'):
console.log('I like to drink water'); 

const drink = age >= 18 ?'wine' :'water';
console.log(drink);

console.log(`i like to drink ${age >= 18 ?'wine' :'water'}`);



