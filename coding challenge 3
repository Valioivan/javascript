let markWeight = 78;
let markHeight = 1.69;

let johnWeight = 92;
let johnHeight = 1.95;

let BMIMark = markWeight / (markHeight * markHeight);
let BMIJohn = johnWeight / (johnHeight * johnHeight);

if (BMIMark > BMIJohn) {
  console.log(`Mark's BMI ${BMIMark} is grater than John's ${BMIJohn}`);
} else {
  console.log(`John's BMI ${BMIJohn} is greate than Mark's ${BMIMark}`);
}
